{
    "name": "robuust/craft-commerce-skrill",
    "description": "Skrill integration for Craft Commerce",
    "version": "2.1.0",
    "type": "craft-plugin",
    "keywords": [
        "skrill",
        "commerce",
        "craftcms",
        "omnipay",
        "yii2"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robuust",
            "homepage": "https://robuust.digital/"
        }
    ],
    "support": {
        "email": "support@robuust.digital",
        "issues": "https://github.com/robuust/craft-commerce-skrill/issues?state=open",
        "source": "https://github.com/robuust/craft-commerce-skrill",
        "docs": "https://github.com/robuust/craft-commerce-skrill",
        "rss": "https://github.com/robuust/craft-commerce-skrill/commits/master.atom"
    },
    "require": {
        "craftcms/cms": "^4.0|^5.0",
        "craftcms/commerce": "^4.0|^5.0",
        "craftcms/commerce-omnipay": "^4.1",
        "samyan/omnipay-skrill": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "robuust\\skrill\\": "src/"
        }
    },
    "extra": {
        "name": "Skrill for Craft Commerce",
        "handle": "commerce-skrill"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
